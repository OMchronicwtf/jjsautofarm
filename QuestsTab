-- AutoQuestTab Loadie
return function(Window, player, QuestEvent, RunService)
    -- Create AutoQuest Tab
    local AutoQuestTab = Window:CreateTab("AutoQuest", 4483362458)
    AutoQuestTab:CreateSection("Quest Controls")

    -- Load Quest Table
    local quests = loadstring(game:HttpGet("https://raw.githubusercontent.com/OMchronicwtf/jjsautofarm/refs/heads/main/Quest%20Table"))()

    -- =======================
    -- AutoQuest Toggle
    -- =======================
    local AutoQuestToggle = loadstring(game:HttpGet("https://raw.githubusercontent.com/OMchronicwtf/jjsautofarm/refs/heads/main/AutoQuests%20ToggleButton"))()
    AutoQuestToggle(player, QuestEvent, quests, RunService, AutoQuestTab)

    -- =======================
    -- Auto Start Specific Quest Toggle
    -- =======================
    local AutoStartSpecificQuest = loadstring(game:HttpGet("https://github.com/OMchronicwtf/jjsautofarm/raw/refs/heads/main/AutoSpecificQuests%20ToggleButton"))()
    AutoStartSpecificQuest(player, QuestEvent, quests, RunService, AutoQuestTab)
end
